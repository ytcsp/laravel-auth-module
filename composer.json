{
    "name": "ytcsp/laravel-auth-module",
    "description": "A comprehensive Laravel authentication package providing login, register, forget password, reset password functionality with middleware",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Strichpunkt",
            "email": "info@strichpunkt.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Strichpunkt\\LaravelAuthModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strichpunkt\\LaravelAuthModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Strichpunkt\\LaravelAuthModule\\AuthModuleServiceProvider"
            ],
            "aliases": {
                "AuthModule": "Strichpunkt\\LaravelAuthModule\\Facades\\AuthModule"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 